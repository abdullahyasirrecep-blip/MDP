#include<stdio.h>

int main() {
    int x;
    int y;
    int z;
    printf("Birinci sayiyi giriniz:");
    scanf("%d",&x);
    printf("Ikinci sayiyi giriniz:");
    scanf("%d",&y);
    printf("Ucuncu sayiyi giriniz:");
    scanf("%d",&z);
    
    if (x <= y && x <= z) {
        if (y <= z) printf("Kucukten buyuye :%d %d %d\n", x, y, z);
        else        printf("Kucukten buyuye :%d %d %d\n", x, z, y);
    }
    else if (y <= x && y <= z) {
        if (x <= z) printf("Kucukten buyuye :%d %d %d\n", y, x, z);
        else        printf("Kucukten buyuye :%d %d %d\n", y, z, x);
    }
    else {
        if (x <= y) printf("Kucukten buyuye :%d %d %d\n", z, x, y);
        else        printf("Kucukten buyuye :%d %d %d\n", z, y, x);
    }

    return 0;
}
